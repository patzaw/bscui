[Term]
id: SL-0457
name: Photoreceptor inner segment
namespace: uniprot:locations:cellular_component
def: "The inner segment of a vertebrate photoreceptor containing mitochondria, ribosomes and membranes where opsin molecules are assembled and passed to be part of the outer segment discs." []
xref: GO:0001917 "photoreceptor inner segment"

[Term]
id: SL-0458
name: Photoreceptor outer segment
namespace: uniprot:locations:cellular_component
