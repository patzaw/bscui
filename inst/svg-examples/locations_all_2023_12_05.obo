synonym: "Presynaptic membrane" [UniProt]
xref: GO:0042734 "presynaptic membrane"
is_a: SL-0531
relationship: part_of SL-0516

[Term]
id: SL-0223
name: Prevacuolar compartment
namespace: uniprot:locations:cellular_component
