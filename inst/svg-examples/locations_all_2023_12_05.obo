relationship: part_of SL-0039
relationship: part_of SL-0298

[Term]
id: SL-0300
name: Ruffle
namespace: uniprot:locations:cellular_component
def: "A ruffle is a cell protrusion at the leading edge of a crawling cell. Ruffles are supported by a microfilament meshwork." []
